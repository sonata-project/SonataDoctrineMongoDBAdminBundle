{
    "name": "sonata-project/doctrine-mongodb-admin-bundle",
    "type": "symfony-bundle",
    "description": "Symfony Sonata / Integrate Doctrine MongoDB ODM into the SonataAdminBundle",
    "keywords": [
        "Admin Generator",
        "admin",
        "sonata",
        "bootstrap",
        "generator",
        "Mongo",
        "MongoDB"
    ],
    "homepage": "http://sonata-project.org/bundles/admin",
    "license": "MIT",
    "authors": [
        {
            "name": "KÃ©vin Dunglas",
            "email": "dunglas@gmail.com",
            "homepage": "http://dunglas.fr"
        },
        {
            "name": "Sonata Community",
            "homepage": "https://github.com/sonata-project/SonataDoctrineMongoDBAdminBundle/contributors"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-mongodb": "*",
        "doctrine/collections": "^1.6",
        "doctrine/mongodb-odm": "^2.1",
        "doctrine/mongodb-odm-bundle": "^4.0",
        "doctrine/persistence": "^1.3.4 || ^2.0",
        "sonata-project/admin-bundle": "^3.78",
        "sonata-project/form-extensions": "^0.1 || ^1.4",
        "symfony/config": "^4.4",
        "symfony/dependency-injection": "^4.4",
        "symfony/doctrine-bridge": "^4.4",
        "symfony/form": "^4.4",
        "symfony/http-kernel": "^4.4",
        "symfony/property-access": "^4.4 || ^5.1",
        "twig/twig": "^2.6 || ^3.0"
    },
    "provide": {
        "sonata-project/admin-bundle-persistency-layer": "1.0.0"
    },
    "require-dev": {
        "doctrine/annotations": "^1.10",
        "doctrine/data-fixtures": "^1.4",
        "matthiasnoback/symfony-dependency-injection-test": "^4.1",
        "phpstan/phpstan": "^0.12.52",
        "symfony/browser-kit": "^4.4 || ^5.1",
        "symfony/css-selector": "^4.4 || ^5.1",
        "symfony/panther": "^0.7.1",
        "symfony/phpunit-bridge": "^5.1.1",
        "vimeo/psalm": "^3.17"
    },
    "suggest": {
        "alcaeus/mongo-php-adapter": "Allows usage of PHP 7"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Sonata\\DoctrineMongoDBAdminBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sonata\\DoctrineMongoDBAdminBundle\\Tests\\": "tests/"
        }
    }
}
