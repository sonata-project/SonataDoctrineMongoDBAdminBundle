{
    "name": "sonata-project/doctrine-mongodb-admin-bundle",
    "type": "symfony-bundle",
    "description": "Symfony Sonata / Integrate Doctrine MongoDB ODM into the SonataAdminBundle",
    "keywords": ["Admin Generator", "admin", "sonata", "bootstrap", "generator", "Mongo", "MongoDB"],
    "homepage": "http://sonata-project.org/bundles/admin",
    "license": "MIT",
    "authors": [
        {
            "name": "KÃ©vin Dunglas",
            "email": "dunglas@gmail.com",
            "homepage": "http://dunglas.fr"
        },
        {
            "name": "Sonata Community",
            "homepage": "https://github.com/sonata-project/SonataDoctrineMongoDBAdminBundle/contributors"
        }
    ],
    "require": {
        "php": "^7.1",
        "doctrine/mongodb-odm": "^1.0",
        "doctrine/mongodb-odm-bundle": "^3.0",
        "sonata-project/admin-bundle": "^3.18",
        "symfony/config": "^2.8 || ^3.2",
        "symfony/dependency-injection": "^2.8 || ^3.2",
        "symfony/doctrine-bridge": "^2.8 || ^3.2",
        "symfony/form": "^2.8 || ^3.2",
        "symfony/http-kernel": "^2.8 || ^3.2"
    },
    "require-dev": {
        "jmikola/geojson": "^1.0",
        "symfony/phpunit-bridge": "^3.3"
    },
    "suggest": {
        "alcaeus/mongo-php-adapter": "Allows usage of PHP 7"
    },
    "provide": {
        "sonata-project/admin-bundle-persistency-layer": "1.0.0"
    },
    "autoload": {
        "psr-4": { "Sonata\\DoctrineMongoDBAdminBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Sonata\\DoctrineMongoDBAdminBundle\\Tests\\": "tests/" }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.x-dev"
        }
    }
}
