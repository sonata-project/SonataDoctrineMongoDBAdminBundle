{
    "name": "sonata-project/doctrine-mongodb-admin-bundle",
    "type": "symfony-bundle",
    "description": "Symfony Sonata / Integrate Doctrine MongoDB ODM into the SonataAdminBundle",
    "keywords": [
        "Admin Generator",
        "admin",
        "sonata",
        "bootstrap",
        "generator",
        "Mongo",
        "MongoDB"
    ],
    "homepage": "http://sonata-project.org/bundles/admin",
    "license": "MIT",
    "authors": [
        {
            "name": "KÃ©vin Dunglas",
            "email": "dunglas@gmail.com",
            "homepage": "http://dunglas.fr"
        },
        {
            "name": "Sonata Community",
            "homepage": "https://github.com/sonata-project/SonataDoctrineMongoDBAdminBundle/contributors"
        }
    ],
    "require": {
        "php": "^7.1",
        "doctrine/mongodb-odm": "^1.3 || ^2.0",
        "doctrine/mongodb-odm-bundle": "^3.0 || ^4.0",
        "sonata-project/admin-bundle": "^3.31",
        "sonata-project/core-bundle": "^3.17",
        "symfony/config": "^3.4 || ^4.2",
        "symfony/dependency-injection": "^3.4 || ^4.2",
        "symfony/doctrine-bridge": "^3.4 || ^4.2",
        "symfony/form": "^3.4 || ^4.2",
        "symfony/http-kernel": "^3.4 || ^4.2",
        "twig/extensions": "^1.0",
        "twig/twig": "^1.34 || ^2.0"
    },
    "provide": {
        "sonata-project/admin-bundle-persistency-layer": "1.0.0"
    },
    "require-dev": {
        "jmikola/geojson": "^1.0",
        "symfony/phpunit-bridge": "^5.0"
    },
    "suggest": {
        "alcaeus/mongo-php-adapter": "Allows usage of PHP 7"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Sonata\\DoctrineMongoDBAdminBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sonata\\DoctrineMongoDBAdminBundle\\Tests\\": "tests/"
        }
    }
}
